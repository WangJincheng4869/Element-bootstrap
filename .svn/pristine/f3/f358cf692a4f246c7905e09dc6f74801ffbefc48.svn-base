/* 文字颜色、超链接复写 */
.text-primary {
    color: #26a7de !important;
}

.text-success {
    color: #67c23a !important;
}

.text-info {
    color: #909399 !important;
}

.text-warning {
    color: #e6a23c !important;
}

.text-danger {
    color: #f56c6c !important;
}

a.link:hover, a.link:focus,
a.link.text-primary:hover, a.link.text-primary:focus {
    position: relative;
    text-decoration: none;
    color: #249bce !important;
}

a.link:hover:after {
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    height: 0;
    bottom: 0;
    border-bottom: 1px solid #249bce;
}

a.link.text-success:hover, a.link.text-success:focus {
    color: #85ce61 !important;
}

a.link.text-success:hover:after {
    border-color: #85ce61;
}

a.link.text-info:hover, a.link.text-info:focus {
    color: #a6a9ad !important;
}

a.link.text-info:hover:after {
    border-color: #a6a9ad;
}

a.link.text-warning:hover, a.link.text-warning:focus {
    color: #ebb563 !important;
}

a.link.text-warning:hover:after {
    border-color: #ebb563;
}

a.link.text-danger:hover, a.link.text-danger:focus {
    color: #f78989 !important;
}

a.text-danger:hover:after {
    border-color: #f78989;
}

.divider-vertical {
    content: "";
    border-left: 1px solid #808080;
    margin: 0 .35rem;
    font-size: .8em;
}

